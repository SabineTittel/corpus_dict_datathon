# lexical entry
:austor a ontolex:LexicalEntry ,
	ontolex:Word ;
  lexinfo:PartOfSpeech "m." ,
  lexinfo:Noun ;
  lexinfo:Gender lexinfo:masculine ;
  ontolex:canonicalForm :austor_form_austor .
:austor_form_austor	a ontolex:Form ;
	ontolex:writtenRep "austor"@pro .
  
