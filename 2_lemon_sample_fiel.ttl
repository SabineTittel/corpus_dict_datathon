					[ ontolex:writtenRep "fieil"@fro ;
 					 lexinfo:partOfSpeech lexinfo:noun ].
:fiel_form_fius a ontolex:Form ;
					[ ontolex:writtenRep "fius"@fro ;
 					 lexinfo:partOfSpeech lexinfo:noun ].

